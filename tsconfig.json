{
  "compilerOptions": {
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "target": "ESNext",
    "lib": [
      "ESNext"
    ],
    "libReplacement": true,
    "exactOptionalPropertyTypes": true,
    "noImplicitOverride": true,
    "noUncheckedIndexedAccess": true,
    "noUncheckedSideEffectImports": true,
    "strict": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "allowJs": true,
    "skipLibCheck": true,
    "paths": {
      "#Utils/*": [
        "./Utils/*"
      ],
      "#Utils/combinedCommands": [
        "./Utils/combinedCommands/*"
      ],
      "#Utils/componentHandler": [
        "./Utils/componentHandler/*"
      ],
      "#Utils/prototypeRegisterer": [
        "./Utils/prototypeRegisterer/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}