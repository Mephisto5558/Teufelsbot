{
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "target": "ESNext",
    "lib": [
      "ESNext"
    ],
    "paths": {
      "#Utils/*": [
        "./Utils/*"
      ],
      "#Utils/combinedCommands": [
        "./Utils/combinedCommands/*"
      ],
      "#Utils/componentHandler": [
        "./Utils/componentHandler/*"
      ],
      "#Utils/prototypeRegisterer": [
        "./Utils/prototypeRegisterer/*"
      ],
      "#types/*": [
        "./types/*.d.ts",
      ],
      "#types/db": [
        "./types/database/index.d.ts"
      ],
      "#types/db/*": [
        "./types/database/*.d.ts"
      ]
    },
    "allowJs": true,
    "exactOptionalPropertyTypes": true,
    "libReplacement": true,
    "noEmit": true,
    "noImplicitOverride": true,
    "noUncheckedIndexedAccess": true,
    "noUncheckedSideEffectImports": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true
  }
}